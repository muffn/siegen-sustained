{
	"name": "siegen-sustained",
	"type": "module",
	"version": "1.1.0",
	"description": "A blog for the siegen-sustained team with Astro",
	"author": "muffn",
	"license": "GPL-3.0-only",
	"repository": {
		"type": "git",
		"url": "https://github.com/muffn/siegen-sustained"
	},
	"keywords": [
		"astro",
		"astrojs",
		"astro-blog",
		"astro-b"
	],
	"bugs": {
		"url": "https://github.com/muffn/siegen-sustained/issues"
	},
	"scripts": {
		"dev": "tinacms dev -c \"astro dev\"",
		"start": "astro dev",
		"build": "astro build",
		"tinacms:build": "tinacms build",
		"sync": "astro sync",
		"preview": "astro preview",
		"postbuild": "pagefind --site dist",
		"format:check": "prettier --plugin-search-dir=. --check .",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "eslint .",
		"pre-commit": "lint-staged"
	},
	"dependencies": {
		"@astrojs/rss": "^4.0.11",
		"@tailwindcss/postcss": "^4.1.8",
		"astro": "^5.3.1",
		"astro-font": "^1.0.0",
		"photoswipe": "^5.4.4",
		"sharp": "^0.33.5",
		"vite": "^6.0.7"
	},
	"devDependencies": {
		"@astrojs/mdx": "^4.0.8",
		"@astrojs/sitemap": "^3.2.1",
		"@pagefind/default-ui": "^1.3.0",
		"@tailwindcss/typography": "0.5.16",
		"@tailwindcss/vite": "^4.1.8",
		"@tinacms/cli": "^1.9.8",
		"@types/node": "^22.13.8",
		"@typescript-eslint/parser": "^8.33.0",
		"clsx": "2.1.1",
		"eslint": "^9.21.0",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"lint-staged": "^15.4.3",
		"mdast-util-to-string": "^4.0.0",
		"motion": "^12.20.1",
		"pagefind": "^1.3.0",
		"prettier": "^3.5.3",
		"prettier-config-standard": "^7.0.0",
		"prettier-plugin-astro": "^0.14.1",
		"reading-time": "^1.5.0",
		"slugify": "^1.6.6",
		"tailwind-merge": "^3.0.2",
		"tailwindcss": "^4.1.8",
		"tinacms": "^2.7.2",
		"typescript": "^5.7.3"
	},
	"lint-staged": {
		"*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
			"prettier --write --plugin-search-dir=."
		]
	}
}
